{
  "name": "tagrelease",
  "version": "0.0.0",
  "description": "Tags a commit using the version found in package.json",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive specs && standard",
    "prepublish": "rm -rf dist && babel --optional runtime lib --out-dir dist --copy-files",
    "postpublish": "rm -rf dist"
  },
  "bin": {
    "tagrelease": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mishguruorg/tagrelease"
  },
  "keywords": [
    "git",
    "version",
    "npm",
    "release",
    "ci",
    "deployment"
  ],
  "author": "Mish Guru <dev@mish.guru>",
  "bugs": {
    "url": "https://github.com/mishguruorg/tagrelease/issues"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.1",
    "chai": "^3.5.0",
    "eslint-plugin-babel": "^3.1.0",
    "mocha": "^2.4.5",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "minimist": "^1.2.0",
    "ramda": "^0.20.1",
    "simple-git": "^1.31.0"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  }
}
